[user]
name = Andrew Smith
email = andrew@andrew.codes
signingkey = ssh-rsa 

[alias]
gui = !sh -c '/usr/local/git/libexec/git-core/git-gui'
co = checkout

[merge]
tool = bc3

[diff]
tool = bc3

[push]
default = simple

[branch]
autosetuprebase = always

[core]
autocrlf = false
editor = code --wait

[filter "media"]
clean = git-media-clean %f
smudge = git-media-smudge %f

[gui]
gcwarning = false

[mergetool]
bc3 = trustExitCode
prompt = false
keepBackup = false

[difftool]
prompt = false

[gpg]
program = /usr/local/bin/gpg
format = ssh

[commit]
gpgsign = true
program = /usr/local/bin/gpg
[init]
defaultBranch = main
