#!/usr/bin/env bash
# Completion for db (delete branch)
_db_complete() {
    local cur branches
    cur="${COMP_WORDS[COMP_CWORD]}"

    # Get all local branches
    branches=$(git branch --format='%(refname:short)' 2>/dev/null)

    COMPREPLY=($(compgen -W "$branches" -- "$cur"))
}

complete -F _db_complete db
